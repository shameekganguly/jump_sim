	        <origin xyz="0 0 0" rpy="0 0 0" />
	        <mass value="1e-9" />
	        <inertia ixx="1e-9" iyy="1e-9" izz="1e-9" ixy="0" ixz="0" iyz="0" />
	    </inertial>
    </link>

