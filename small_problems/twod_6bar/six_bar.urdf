		</inertial>
        <visual>
            <origin xyz="-0.2 0.0 0.0" rpy="0 0 0" />
            <geometry>
                <box size="0.4 0.4 0.2"/>
            </geometry>
            <material name="material_maroon">
                <color rgba="0.6 0.2 0.3 1.0" />
            </material>
        </visual>
    </link>
    <link name="right_thigh">
